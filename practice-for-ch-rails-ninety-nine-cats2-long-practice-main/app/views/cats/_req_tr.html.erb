<tr>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
    <% if req.pending? %>
      <% if owner_logged_in? %>
        <form action="<%= approve_cat_rental_request_url(req) %>" method="post">
            <input type="submit" value="Approve">
        </form>
        <form action="<%= deny_cat_rental_request_url(req) %>" method="post">
            <input type="submit" value="Deny">
        </form>
      <% else %>
        PENDING
      <% end %>
    <% else %>
      <%= req.status %>
    <% end %>
  </td>
</tr>
